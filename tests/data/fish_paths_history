- cmd: echo hello
  when: 1700000001
- cmd: echo hello world
  when: 1700000002
- cmd: cargo build
  when: 1700000003
- cmd: echo foo\\\nbar\\\nbaz
  when: 1700000004
- cmd: \n
  when: 1700000005
- cmd: echo foo\necho bar
  when: 1700000006
- cmd: echo foo\\\\bar
  when: 1700000007
- cmd: echo foo\\\n: 42:0;echo bar
  when: 1700000008
- cmd: echo foo\\nbar
  when: 1700000009
- cmd: echo foo \\ bar
  when: 1700000010
- cmd: echo foo \\\\ bar
  when: 1700000011
- cmd: ls ~
  when: 1800000001
  paths:
    - ~
- cmd: ls /bin/ /sbin
  when: 1800000002
  paths:
    - /bin/
    - /sbin
- cmd: ls "$HOME"
  when: 1800000003
  paths:
    - "$HOME"
- cmd: tail ~/.local/share/fish/fish_history
  when: 1800000004
  paths:
    - ~/.local/share/fish/fish_history
- cmd: cat 'foo \\ bar.txt'
  when: 1800000005
  paths:
    - 'foo \\ bar.txt'
